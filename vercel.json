{
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "frontend",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/cpf",
      "dest": "index.ts"
    },
    {
      "src": "/cnpj-alfanumerico",
      "dest": "index.ts"
    },
    {
      "src": "/cnpj-numerico",
      "dest": "index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dist/$1"
    }
  ]
}
