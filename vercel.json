{
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/cpf",
      "dest": "index.ts"
    },
    {
      "src": "/api/cnpj-alfanumerico",
      "dest": "index.ts"
    },
    {
      "src": "/api/cnpj-numerico",
      "dest": "index.ts"
    },
    {
      "src": "/api",
      "dest": "index.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "frontend/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dist/index.html"
    }
  ]
}
